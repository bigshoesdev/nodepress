                        <div class="recommnet">
                          <div class="recommnet-img">
                            <img src="<%= post[i*3 + j].file %>">
                          </div>
                          <div class="recommnet-cate">
                            <a href="javascript:void(0);"
                              style="font-family: Montserrat;"><span><%= post[i*3 + j].category.name%></span></a>
                          </div>
