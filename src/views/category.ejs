                            <p style="overflow-wrap: break-word;font-size: 18px;">
                              <%
                                var showPopup = true;
                                var url = "";
                                if(post[i*3 + j].postedBy.roleId == "admin"){
                                  url = "/d/" + post[i*3 + j].category.slug + "/" + post[i*3 + j].slug;
                                }else{
                                  url = "/p/" + post[i*3 + j].category.slug + "/" + post[i*3 + j].slug;
                                }
                                if(post[i*3 + j].qualify != "qualify"){
                                  showPopup = false;
                                }
