              </div>
              <div id="myTopnav" class="topnav">
                <form action="/search" method="get" class="search-form d-lg-flex" id="searchForm">
                  <div class="container">
                    <a type="button" href="javascript:void(0)" class="closebtn"
                      style="right: 15%; padding-left: 10px; padding-right: 10px;" onclick="searchForm()"
                      id="closebtn"><i class="fa fa-search"></i></a>
                    <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
                    <input id="q" type="text" placeholder="Suche" name="q" value="" onfocus="displaySearchbtn()" />
                    <label>Suche Inhalte, die dich begeistern</label>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </section>
        <a class="searchbar" href="/category"><span style="padding-left: 10px;cursor:pointer">Explorer</span></a>
        <a class="searchbar" href="#"><span style="font-size:20px; padding-left: 10px;cursor:pointer"
            onclick="openNav()"><i class="fa fa-search" aria-hidden="true"></i></span></a>
        <% if(user) { %>
        <div class="dropdown">
          <a class="dropdown-toggle" type="button" data-toggle="dropdown" style="font-size: 20px;"><i
              class="fa fa-user"></i><span class="caret"></span></a>
          <ul class="dropdown-menu profile">
            <% if(user.roleId == 'admin') { %>
            <li><span><%= user.username%></span></li>
            <li><a href="/dashboard/index" class="searchbar"><i class="fa fa-cog profileicon"></i>Control Panel</a></li>
            <li><a href="/author/<%= user.username%>" class="searchbar"><i class="fa fa-user-circle profileicon"></i>Profile</a></li>
            <li><a href="/log-out" class="searchbar"><i class="fa fa-sign-out profileicon"></i>Sign Out</a></li>
          </ul>
          <% } else {%>
          <li><span><%= user.username%></span></li>
          <li><a href="/user/dashboard" class="searchbar"><i class="fa fa-cog profileicon"></i>Control Panel</a></li>
          <li><a href="/author/<%= user.username%>" class="searchbar"><i class="fa fa-user-circle profileicon"></i>Profile</a></li>
          <li><a href="/log-out" class="searchbar"><i class="fa fa-sign-out profileicon"></i>Sign Out</a></li>
          <% } %>
          </ul>
        </div>
        <% } else {%>
        <a href="/sign-up" class="searchbar"><span style="font-size:20px; padding-left: 10px;cursor:pointer"><i
              class="fa fa-user"></i></span></a>
        <a href="/login" class="searchbar"><span style="font-size:20px; padding-left: 10px;cursor:pointer"><i
              class="fa fa-sign-in"></i></span></a>
        <%}%>
        <span class="webtoggle" style="font-size:24px;cursor:pointer" onclick="openaav()">&#9776; </span>
        <span class="mobiletoggle" style="font-size:24px; margin-right: 20px;cursor:pointer" onclick="openAav()">&#9776;
        </span>
        <div id="myRightnav" class="rightnav">
          <a href="javascript:void(0)" class="closebtn webcros" onclick="closeaav()">&times;</a>
          <a href="javascript:void(0)" class="closebtn mobilecros" onclick="closeAav()">&times;</a>
          <a href="#"><span>Home</span></a>
          <a href="#"><span>The News</span></a>
          <a href="#"><span>Contact US</span></a>
          <a href="#"><span>Demos</span></a>
