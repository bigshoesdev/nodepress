<!-- list top featured row start -->
<div class="list_top_featured">
   <section class="container">
      <div class="row featured_inner">
         <%  var editorsPickerLength = editorsPicker.length;
         if(editorsPicker.length > 3) {
            editorsPickerLength = 3;
         }
         %>
         <% for(var i = 0; i < editorsPickerLength; i ++){%>
         <div class="col-md-12 col-lg-4">
            <%
             var url = "#";
             if(editorsPicker[i].postedBy.roleId == "admin"){
               url = "/d/" + editorsPicker[i].category.slug + "/" + editorsPicker[i].slug;
             }else {
               url = "/p/" + editorsPicker[i].category.slug + "/" + editorsPicker[i].slug;
             }
            %>
            <div class="img-fluid dein-image" url="<%= url%>" style="background-image: url('<%= editorsPicker[i].file %>');">
            </div>
            <a class="underline" target="_blank" href="/kategorie/<%= editorsPicker[i].category.slug%>" />
               <p><%= editorsPicker[i].category.name%></p>
            </a>
            <h3><a href="<%= url%>" target="_blank"><%= editorsPicker[i].title%></a></h3>
         </div>
         <% } %>
      </div>
      <div class="row">
         <% var length = categories.length;
            if(length > 6){ length = 6;}
         %>
         <% for(var i = 0; i < length; i ++) {%>
         <div class="col-sm-12 col-md-6 col-lg-4 featured_travel">
            <a href="/kategorie/<%= categories[i].slug%>" target="_blank">
               <span class="fea_travel_img">
                  <img class="img-fluid" 
                  src="<%= categories[i].background%>"></span>
               <span class="fea_travel_con">
                  <p class="underline"><%= categories[i].name%></p>
                  <!-- <h6><%= categories[i].description%></h6> -->
               </span>
            </a>
         </div>
         <% } %>
      </div>
   </section>
</div>
<!-- Multiple Visible Slider start -->
<div class="multiple_visible_slider">
